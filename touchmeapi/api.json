{
    "title": "TOUCH ME-API DOCUMENTACIÓN",
    "baseurl": "https://grindhood.com/touchme-api",
    "services": [
        {
            "protected": false,
            "url": "/player",
            "method": "POST",
            "title": "NUEVO JUGADOR",
            "description": "Registro de un nuevo jugador",
            "request": {
                "nombres": "Daniela",
                "correo": "dani@gmail.com"
            },
            "success": {
                "message": "Solo usar si es la primera vez jugando",
                "json": {
                    "code": 200,
                    "devmsg": "creado",
                    "usrmsg": "Genial... ya puedes jugar",
                    "content": {
                        "nombres": "Daniela",
                        "correo": "dani@gmail.com",
                        "id": 1
                    }
                }
            },
            "warning": [
                {
                    "message": "Si el email ya está siendo usado",
                    "json": {
                        "code": 401,
                        "devmsg": "El correo ya fue registrado",
                        "usrmsg": "Parece que ya han jugado con este correo antes"
                    }
                }
            ],
            "error": [
                {
                    "message": "si los campos son invalidos",
                    "json": {
                        "code": 400,
                        "devmsg": "No se reconocen uno o varios de los campos: nombres, correo",
                        "usrmsg": "Error desconocido, por favor contacte al desarrollador, codigo: 400"
                    }
                }
            ]
        },
        {
            "protected": false,
            "url": "/email",
            "method": "POST",
            "title": "LOGIN EMAIL",
            "description": "Ruta para volver a jugar usando el email.",
            "request": {
                "correo": "dani@gmail.com"
            },
            "success": {
                "message": "retornamos los datos del usuario",
                "json": {
                    "code": 200,
                    "devmsg": "creado",
                    "usrmsg": "Genial... ya puedes jugar",
                    "content": {
                        "id": 1,
                        "nombres": "Daniela",
                        "correo": "dani@gmail.com"
                    }
                }
            },
            "warning": [
                {
                    "message": "Si el correo no esta siendo usado.",
                    "json": {
                        "code": 401,
                        "devmsg": "El correo no esta registrado",
                        "usrmsg": "Ups, registra tu nombre para poder jugar"
                    }
                }
            ],
            "error": [
                {
                    "message": "si fallan los campos",
                    "json": {
                        "code": 400,
                        "devmsg": "No se reconocen uno o varios de los campos: correo",
                        "usrmsg": "Error desconocido, por favor contacte al desarrollador, codigo: 400"
                    }
                }
            ]
        },
        {
            "protected": false,
            "url": "/record",
            "method": "POST",
            "title": "NUEVO SCORE",
            "description": "Ruta para registrar un score de un jugador.",
            "request": {
                "id": 1,
                "score": 16
            },
            "success": {
                "message": "Si todo fue bien.",
                "json": {
                    "code": 200,
                    "devmsg": "todo OK",
                    "usrmsg": "Genial, siempre puedes intentarlo otra vez",
                    "content": 1
                }
            },
            "warning": [],
            "error": [
                {
                    "message": "si el id no existe",
                    "json": {
                        "code": 400,
                        "devmsg": "No existe el ID:0",
                        "usrmsg": "Error desconocido, por favor contacte al desarrollador, codigo: 400"
                    }
                }
            ]
        },
        {
            "protected": false,
